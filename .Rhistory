test_that("double_down removes correct card from deck", {
hand <- c("8", "2")
deck <- c("K", "3", "4")
result <- double_down(hand, deck)
expect_equal(result$new_hand[3], "K")
expect_equal(result$deck, c("3", "4"))
})
